{
  "name": "watermark-extension",
  "version": "5.5.0",
  "description": "Watermark to PDF extension for Docsvision WebClient.",
  "scripts": {
    "build": " rollup -c rollup.config.js && gulp styles",
    "build:prod": "SET buildTarget=PROD&& npm run build",
    "watch": "concurrently \"rollup -c rollup.config.js --watch\" \"gulp watch\""
  },
  "author": "Docsvision",
  "dependencies": {
    "gulp": "^4.0.2",
    "rollup": "^2.33.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DocsVision/WebClient-Samples.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@docsvision/webclient": "~5.16.1",
    "@docsvision/webclient-extension-build": "~5.16.1",
    "concurrently": "^4.1.1"
  }
}
