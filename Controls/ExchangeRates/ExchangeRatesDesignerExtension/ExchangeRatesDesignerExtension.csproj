      <HintPath>..\..\..\Assemblies\Docsvision\DocsVision.Platform.StorageServer.dll</HintPath>
      <Private>False</Private>
    </Reference>
    <Reference Include="DotNetProjects.Wpf.Extended.Toolkit">
