    </Reference>
    <Reference Include="WebLayoutsDesigner">
      <HintPath>..\..\..\Assemblies\Docsvision\WebLayoutsDesigner.dll</HintPath>
    </Reference>
  </ItemGroup>
  <ItemGroup>
